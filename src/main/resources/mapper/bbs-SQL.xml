<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ksj.bbs.mapper.BbsMapper">

    <!-- 게시판 관리 목록 조회 -->
    <select id="selectBbsMasterListPage" parameterType="BbsVO" resultType="BbsVO">
        select bbs_id, bbs_name, bbs_reply_yn, bbs_file_yn, bbs_file_size, bbs_new_hour, bbs_writer_view_yn
        from bbs_master
        order by bbs_id
    </select>
</mapper>